{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "delete",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "delete": {
      "projectType": "application",
      "root": "apps/delete",
      "sourceRoot": "apps/delete/src",
      "prefix": "angular-track-by",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/delete",
            "index": "apps/delete/src/index.html",
            "main": "apps/delete/src/main.ts",
            "polyfills": "apps/delete/src/polyfills.ts",
            "tsConfig": "apps/delete/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/delete/src/favicon.ico", "apps/delete/src/assets"],
            "styles": ["apps/delete/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/delete/src/environments/environment.ts",
                  "with": "apps/delete/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "delete:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "delete:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "delete:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/delete/src/**/*.ts",
              "apps/delete/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/delete"],
          "options": {
            "jestConfig": "apps/delete/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "delete-e2e": {
      "root": "apps/delete-e2e",
      "sourceRoot": "apps/delete-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/delete-e2e/cypress.json",
            "tsConfig": "apps/delete-e2e/tsconfig.e2e.json",
            "devServerTarget": "delete:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "delete:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/delete-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "angular-track-by": {
      "projectType": "library",
      "root": "libs/angular-track-by",
      "sourceRoot": "libs/angular-track-by/src",
      "prefix": "angular-track-by",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/angular-track-by/tsconfig.lib.json",
            "project": "libs/angular-track-by/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/angular-track-by/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular-track-by/src/**/*.ts",
              "libs/angular-track-by/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/angular-track-by"],
          "options": {
            "jestConfig": "libs/angular-track-by/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
